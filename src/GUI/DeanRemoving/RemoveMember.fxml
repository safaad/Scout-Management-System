<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="284.0" prefWidth="398.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.DeanRemoving.RemoveMemberController">
   <children>
      <TextField fx:id="id" layoutX="152.0" layoutY="85.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label layoutX="14.0" layoutY="88.0" text="Member ID:" textFill="#3f157e">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button layoutX="300.0" layoutY="206.0" mnemonicParsing="false" onAction="#submit" style="-fx-background-color: #3f157e;" text="Submit" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="done" layoutX="7.0" layoutY="43.0" text="The Member successfuly Removed" textFill="#3700ff" visible="false">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="error" layoutX="121.0" layoutY="142.0" text="No Member ID" textFill="RED" visible="false">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
