<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="284.0" prefWidth="398.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.DeanRemoving.RemoveLeaderController">
   <children>
      <TextField fx:id="id" layoutX="152.0" layoutY="56.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label layoutX="14.0" layoutY="56.0" text="Leader ID:" textFill="#3f157e">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button layoutX="300.0" layoutY="206.0" mnemonicParsing="false" onAction="#Done" style="-fx-background-color: #3f157e;" text="Submit" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="14.0" layoutY="97.0" text="Remove Leader Members ?" textFill="#3f157e">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <RadioButton fx:id="yes" layoutX="99.0" layoutY="142.0" mnemonicParsing="false" text="yes">
         <font>
            <Font size="18.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="hell" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="no" layoutX="199.0" layoutY="142.0" mnemonicParsing="false" selected="true" text="No" toggleGroup="$hell">
         <font>
            <Font size="18.0" />
         </font>
      </RadioButton>
      <Label fx:id="error" layoutX="40.0" layoutY="176.0" text="Please check the above info " textFill="RED" visible="false">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="done" layoutX="1.0" layoutY="16.0" text="The Leader is successfuly removed" textFill="#5e00ff" visible="false">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
